#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int num;
    scanf("%d", &num);
    
    if(num % 3 == 0)
       printf("The number is divisible by 3\n");
     else
         printf("The number is not divisible by 3 and gives a remainder %d\n",num %3);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
